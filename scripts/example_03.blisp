; Example #3 - List Processing

(DEFINE LST1 (LIST 1 2 3 4 5))
(PRINTLN "List 1: " LST1)

; List transformations using the map procedure
(PRINTLN "Mapping over List 1: " (MAP (LAMBDA (N) (+ N 2)) LST1))

; List transformations using the filter procedure
(PRINTLN "Filtering List 1: " (FILTER (LAMBDA (N) (> N 2)) LST1))

; Accumulating a result using the reduce procedure
(PRINTLN "Reducing List 1: " (REDUCE + 0 LST1))

; Summing the even numbers up to 10
(REDUCE + 0 (RANGE 0 (INC 10) 2))
